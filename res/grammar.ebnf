   <script> -> <imports> <funcdecls>
  <imports> -> <import> <imports>
             | ε
   <import> -> string -> id stmtend
<funcdecls> -> <funcdecl> <funcdecls>
             | ε
 <funcdecl> -> id <argdecls> := <expr> stmtend
 <argdecls> -> id <argdecls>
             | ε
     <expr> -> <func> <args>
             | { <exprs> }
             | switch <expr> { <switches> }
     <args> -> <single> <args>
             | ε
    <exprs> -> <expr> stmtend <exprs>
             | ε
 <switches> -> <expr> := <expr> stmtend <switches>
             | default := <expr> stmtend <switches>
             | ε
   <single> -> <func>
             | number
             | string
             | <array>
             | { <exprs> }
    <array> -> [ <exprs> ]
     <func> -> id <subfunc>
  <subfunc> -> . id
             | ε

# vim: et
