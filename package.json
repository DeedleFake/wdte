{
  "name": "wdte-playground",
  "version": "0.1.0",
  "description": "WDTE Playground",
  "private": true,
  "scripts": {
    "build": "run-s -l go:build react:build",
    "deploy": "gh-pages -d build",
    "clean": "rimraf build public/wdte.wasm",
    "go:build": "GOOS=js GOARCH=wasm go build -v -o public/wdte.wasm ./go",
    "react:start": "BROWSER=none react-scripts start",
    "react:build": "react-scripts build",
    "prettier": "find src public -iname '*.js' -exec prettier --write {} +",
    "predeploy": "run-s build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DeedleFake/wdte.git"
  },
  "author": "DeedleFake",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DeedleFake/wdte/issues"
  },
  "homepage": "https://deedlefake.github.io/wdte",
  "devDependencies": {
    "@material-ui/styles": "^4.1.2",
    "brace": "^0.11.1",
    "gh-pages": "^2.0.1",
    "jss-preset-default": "^4.5.0",
    "npm-run-all": "^4.1.5",
    "pako": "^1.0.10",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-ace": "^7.0.2",
    "react-dom": "^16.8.6",
    "react-markdown": "^4.0.9",
    "react-scripts": "^3.0.1",
    "react-transition-group": "^4.1.1",
    "rimraf": "^2.6.3",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.87.2"
  },
  "prettier": {
    "arrowParens": "always",
    "parser": "flow",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "useTabs": true
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
