{
  "name": "wdte-playground",
  "version": "0.1.0",
  "description": "WDTE Playground",
  "private": true,
  "scripts": {
    "build": "run-s -l go:build react:build",
    "deploy": "gh-pages -d build",
    "clean": "rimraf build public/wdte.wasm",
    "go:build": "GOOS=js GOARCH=wasm go build -v -o public/wdte.wasm ./go",
    "react:start": "BROWSER=none react-scripts start",
    "react:build": "react-scripts build",
    "prettier": "find src public -iname '*.js' -exec prettier --write {} +",
    "predeploy": "run-s build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DeedleFake/wdte.git"
  },
  "author": "DeedleFake",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DeedleFake/wdte/issues"
  },
  "homepage": "https://deedlefake.github.io/wdte",
  "dependencies": {
    "brace": "^0.11.1",
    "clipboard": "^2.0.4",
    "jss-preset-default": "^10.0.0-alpha.7",
    "react": "^16.7.0",
    "react-ace": "^6.3.2",
    "react-dom": "^16.7.0",
    "react-jss": "^8.6.1",
    "react-markdown": "^4.0.6",
    "react-transition-group": "^2.5.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.84.0"
  },
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "react-scripts": "^2.1.3",
    "rimraf": "^2.6.3"
  },
  "prettier": {
    "arrowParens": "always",
    "parser": "flow",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "useTabs": true
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
