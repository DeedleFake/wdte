{
	"name": "wdte-playground",
	"version": "0.1.0",
	"description": "WDTE Playground",
	"scripts": {
		"build-go": "GO111MODULE=off GOOS=js GOARCH=wasm go1.11beta3 build -v -o public/wdte.wasm ./go",
		"start": "BROWSER=none react-scripts start",
		"prebuild": "npm run build-go",
		"build": "react-scripts build",
		"predeploy": "npm run build",
		"deploy": "gh-pages -d build",
		"prettier": "find src public -iname '*.js' -exec prettier --write {} +"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/DeedleFake/wdte.git"
	},
	"author": "DeedleFake",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/DeedleFake/wdte/issues"
	},
	"homepage": "https://deedlefake.github.io/wdte",
	"dependencies": {
		"brace": "^0.11.1",
		"react": "^16.4.2",
		"react-ace": "^6.1.4",
		"react-dom": "^16.4.2",
		"react-jss": "^8.6.1",
		"react-markdown": "^3.4.1",
		"semantic-ui-css": "^2.3.3",
		"semantic-ui-react": "^0.82.2"
	},
	"devDependencies": {
		"gh-pages": "^1.2.0",
		"prettier": "^1.14.2",
		"react-scripts": "^1.1.4"
	},
	"prettier": {
		"arrowParens": "always",
		"parser": "flow",
		"semi": false,
		"singleQuote": true,
		"trailingComma": "all",
		"useTabs": true
	}
}
